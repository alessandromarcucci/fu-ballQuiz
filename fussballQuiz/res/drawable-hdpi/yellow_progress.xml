<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background">
    <shape>
        <corners android:radius="45dip" />
        <solid android:color="#276350" />

        <stroke
            android:width="1dp"
            android:color="#276350" />
        <gradient
                android:startColor="#276350"
                android:centerColor="#276350"
                android:centerY="0.75"
                android:endColor="#276350"
                android:angle="270"
        />
    </shape>
</item>

<item android:id="@android:id/secondaryProgress">
    <clip>
        <shape>
            <corners android:radius="15dip" />
            <gradient
                    android:startColor="#ffffff"
                    android:centerColor="#ffffff"
                    android:centerY="0.75"
                    android:endColor="#ffffff"
                    android:angle="270"
                    
            />
          
        </shape>
    </clip>
</item>
<item
    android:id="@android:id/progress"
>
    <clip>
        <shape>
            <corners
                android:radius="45dip" />
            <gradient
                android:startColor="#f9b841"
                android:endColor="#f9b841"
                android:angle="270" />
        </shape>
    </clip>
</item>

</layer-list>
